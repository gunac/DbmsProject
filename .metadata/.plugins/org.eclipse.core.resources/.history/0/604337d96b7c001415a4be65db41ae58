	public List<Rental> getAllRental() {
		em.getTransaction().begin();
		Query q = em.createQuery("SELECT r FROM Rental r", Rental.class);
		List<Rental> lstRentalInfo = q.getResultList();
		em.getTransaction().commit();
		return lstRentalInfo;
